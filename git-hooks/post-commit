#!/bin/sh
if [ -a .commit ]
    then
    rm .commit
    git add $(git rev-parse --show-toplevel)/fme-tour.coffee
    git add $(git rev-parse --show-toplevel)/spec/fme-tour-spec.coffee
    git add $(git rev-parse --show-toplevel)/karma_tests.html
    git add $(git rev-parse --show-toplevel)/git-hooks/.
    git commit --amend -C HEAD --no-verify
fi
exit 0